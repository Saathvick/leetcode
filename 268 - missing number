class Solution {
    public int missingNumber(int[] nums) {
                    int ans = 0;
                    int sum = 0;
        for(int i=0;i<nums.length;i++){
            sum += nums[i];
        }
            int len = nums.length;
            int f = len * (len + 1) / 2;
            ans = f - sum;
            return ans;
         
    }
}
